    "express": "~4.16.1",
    "fs-extra": "^11.3.0",
    "http-errors": "~1.6.3",
    "multer": "^2.0.2",
    "open": "^8.4.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
